{
    "name": "masterflash-ru/users",
    "description": "Система авторизации посетителя сайта и администраторов",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "masterflash-ru/ado": "^2.2",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-authentication": "^2.5",
        "laminas/laminas-crypt": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Users\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Users\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Users",
            "config-provider": "masterflash-ru\\Mf\\Users\\ConfigProvider"
        }
    }
}
